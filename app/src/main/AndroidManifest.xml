<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="info.androidhive.recyclerviewswipe">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".list_androidhive_swipe_drag_and_drop.AndroidhiveActivity" />
        <activity android:name=".list_swipe_drag_and_drop.ListActivity" />
        <activity android:name=".gid_swipe_drag_and_drop.GridActivity" />
        <activity android:name=".decoration.DecorationActivity" />
        <activity android:name=".nestle.NestleActivity" />
        <activity android:name=".loadmore.LoadMoreActivity" />
        <activity android:name=".header_grid.GridHeaderActivity"></activity>
    </application>

</manifest>